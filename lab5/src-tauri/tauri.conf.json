{
	"plugins": {
		"http": {
			"enabled": true
		}
	},
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"productName": "Лёгкая Жизнь - Расчет ДЖЕЛ",
	"version": "0.1.0",
	"identifier": "com.lunglife.app",
	"build": {
		"frontendDist": "../dist",
		"devUrl": "https://169.254.54.75:3000",
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build"
	},
	"app": {
		"security": {
			"csp": null
		},
		"windows": [
			{
				"title": "Лёгкая Жизнь - Расчет ДЖЕЛ",
				"width": 1200,
				"height": 800,
				"resizable": true,
				"devtools": true
			}
		],
		"withGlobalTauri": true
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
